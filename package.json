{
	"name": "canonn_edsm_updater",
	"version": "0.1.0",
	"description": "Nodejs based tool to update the Canonn API with planetary and stellar data from EDSM",
	"main": "index.js",
	"scripts": {
		"status": "nodemon --exec babel-node src/index.js status",
		"updateSystems": "nodemon --exec babel-node src/index.js updateSystems",
		"updateBodies": "nodemon --exec babel-node src/index.js updateBodies",
		"updateAll": "nodemon --exec babel-node src/index.js updateAll"
	},
	"author": "Vall (Vall @ Canonn discord)",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1",
		"nodemon": "^1.18.7"
	},
	"dependencies": {
		"dotenv": "^6.1.0",
		"node-fetch": "^2.3.0"
	}
}
